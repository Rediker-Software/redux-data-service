{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "declaration": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "removeComments": true,
    "strictNullChecks": false,
    "outDir": "../../test-dist",
    "skipLibCheck": true,
    "lib": [
      "es6",
      "es7",
      "es2017",
      "dom"
    ]
  },
  "types": [
    "intern"
  ]
}
